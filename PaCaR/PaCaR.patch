+			     const char *buf, size_t len)
+{
+	if (tolower(*buf) == '0') {
+		duplication_memory_pressure_mitigation = false;
+	} else if (tolower(*buf) == '1') {
+		duplication_memory_pressure_mitigation = true;
+	} else
+		return -EINVAL;
+
+	return len;
+}
